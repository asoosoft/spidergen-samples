<div class="AView-Style" data-base="AView" data-class="ListItemView" style="left: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(51, 51, 51); border-bottom: 1px solid rgb(197, 197, 197);" data-browser-width="600" data-browser-height="80" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ALabel&quot;,&quot;AButton&quot;,&quot;AImage&quot;]}">
    <label class="ALabel-Style" data-base="ALabel" data-class="ALabel" data-group="" data-sgap-width="1" data-stretch-width="true" id="ListItemView--itemLabel" style="width: calc((100% - 70px) - 90px); height: 50px; left: 70px; font-weight: normal; font-size: 15px; line-height: 50px; color: rgb(255, 255, 255); top: calc(50% - 25px);" data-center-y="true"></label>
    <button class="AButton-Style" data-base="AButton" data-class="AButton" data-flag="0001" data-group="" id="ListItemView--deleteButton" style="width: 70px; right: 10px; top: calc(50% - 19px); height: 40px;" data-center-y="true" data-listener-click="ListItemView:onDeleteButtonClick">Delete</button>
    <img class="AImage-Style aimage-blank" data-base="AImage" data-class="AImage" data-flag="0001" data-group="" id="ListItemView--itemImage" style="width: 40px; height: 40px; left: 10px; top: calc(50% - 20px);" data-center-y="true">
</div>